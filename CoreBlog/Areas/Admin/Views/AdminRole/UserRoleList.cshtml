@model List<EntityLayer.Concrete.AppUser>
@{
    ViewData["Title"] = "UserRoleList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Kullanıcı</th>
            <th>Rol Ata</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td><a href="/Admin/AdminRole/AssignRole/@item.Id" class="btn btn-succes">Ata</a></td>
            </tr>
        }
    </tbody>
</table>
<a href="/Admin/AdminRole/Index" class="btn btn-primary">Rol Listesine Dön</a>