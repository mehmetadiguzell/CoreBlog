@using CoreBlog.Controllers
@model List<Class1>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>İsim</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td><a href="/EmployeTest/DeleteEmploye/@item.Id" class="btn btn-danger">Sil</a></td>
            <td><a href="/EmployeTest/EditEmployeAsync/@item.Id" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
   <a href="/EmployeTest/AddEmploye" class="btn btn-warning">Düzenle</a>
